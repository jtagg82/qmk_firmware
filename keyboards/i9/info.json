    "bootloader_instructions": "1. Press reset through hole on back of keyboard, 2. Hold both SHIFT keys and press B.",
    "usb": {
        "vid": "0xFEED",
        "pid": "0x1234",
        "device_version": "1.0.0",
        "force_nkro": true
    },
    "bootloader": "caterina",
    "develoopment_board": "promicro",
